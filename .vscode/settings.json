{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": [
    "source.fixAll.stylelint",
    "source.fixAll.eslint"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "stylelint.enable": true,
  "stylelint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  }
}
